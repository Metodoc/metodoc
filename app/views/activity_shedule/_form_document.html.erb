<%@atividades = @documento.activity_shedule%>

<div style="margin-left: 10px;">
    <table>
        <%@atividades.each do |a|%>
        <tr id="tr<%=a.id%>">
            <td><%=a.name%></td>
            <td><%=a.description%></td>
            <td><%=a.deadline%></td>
        </tr>
        <%end%>

    </table>
</div>
